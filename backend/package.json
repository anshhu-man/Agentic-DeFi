{
  "name": "agentic-meta-protocol-explorer-backend",
  "version": "1.0.0",
  "description": "Backend for Agentic Meta-Protocol Explorer - AI-powered DeFi intelligence platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:3000": "PORT=3000 nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:3001": "PORT=3001 nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "test:preflight": "node test-preflight-check.js",
    "test:api-simple": "node test-api-endpoints-simple.js",
    "test:analyze-endpoint": "node test-analyze-endpoint.js",
    "test:analyze-only": "node test-analyze-only-comprehensive.js",
    "test:query-formats": "node test-query-response-formats.js",
    "test:direct-query": "node test-query-direct.js",
    "test:execute-approval": "node test-execute-approval-comprehensive.js",
    "test:execute-with-approval": "node test-execute-with-approval.js",
    "test:1inch-enhanced": "node test-enhanced-1inch-integration.js",
    "pipeline:3000": "zsh -c 'PORT=3000 npm run dev & echo $! > .server3000.pid && node test-server-ready.js && npm run test:api-simple && npm run test:query-formats; kill -TERM $(cat .server3000.pid) && rm .server3000.pid'",
    "pipeline:3001": "zsh -c 'PORT=3001 npm run dev & echo $! > .server3001.pid && sleep 3 && PORT=3001 npm run test:analyze-endpoint; kill -TERM $(cat .server3001.pid) && rm .server3001.pid'",
    "pipeline:analyze-only": "npm run test:analyze-only",
    "pipeline:agents": "zsh -c 'npm run test:direct-query && npm run test:1inch-enhanced && npm run test:execute-approval && npm run test:execute-with-approval'",
    "pipeline:all": "zsh -c 'npm run build && npm run test:preflight && npm run pipeline:3000 && npm run pipeline:3001 && npm run pipeline:analyze-only && npm run pipeline:agents'"
  },
  "keywords": [
    "defi",
    "ai",
    "blockchain",
    "web3",
    "agents"
  ],
  "author": "EthIndia Hackathon Team",
  "license": "MIT",
  "dependencies": {
    "@mistralai/mistralai": "^1.10.0",
    "@pythnetwork/client": "^2.19.0",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@pythnetwork/pyth-evm-js": "^2.0.0-alpha2",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "helmet": "^7.0.0",
    "node-cron": "^3.0.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.9",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
