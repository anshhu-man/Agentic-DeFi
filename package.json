    "@prisma/client": "^5.2.0",
    "@pythnetwork/client": "^2.19.0",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@pythnetwork/pyth-evm-js": "^2.0.0-alpha2",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^5.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
