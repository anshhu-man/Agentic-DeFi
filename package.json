  "description": "Backend for Agentic Meta-Protocol Explorer - AI-powered DeFi intelligence platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
